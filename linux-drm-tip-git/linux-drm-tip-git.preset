# mkinitcpio preset file for the 'linux-drm-tip-git' package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux-drm-tip-git"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux-drm-tip-git.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-linux-drm-tip-git-fallback.img"
fallback_options="-S autodetect"
